{
    "C_Cpp.default.includePath": [
        "${workspaceFolder}/src/include",
        "${workspaceFolder}/duckdb/src/include",
        "${workspaceFolder}/duckdb/third_party/fsst",
        "${workspaceFolder}/duckdb/third_party/fmt/include",
        "${workspaceFolder}/duckdb/third_party/hyperloglog",
        "${workspaceFolder}/duckdb/third_party/fastpforlib",
        "${workspaceFolder}/duckdb/third_party/skiplist",
        "${workspaceFolder}/duckdb/third_party/ska_sort",
        "${workspaceFolder}/duckdb/third_party/fast_float",
        "${workspaceFolder}/duckdb/third_party/re2",
        "${workspaceFolder}/duckdb/third_party/miniz",
        "${workspaceFolder}/duckdb/third_party/utf8proc/include",
        "${workspaceFolder}/duckdb/third_party/concurrentqueue",
        "${workspaceFolder}/duckdb/third_party/pcg",
        "${workspaceFolder}/duckdb/third_party/pdqsort",
        "${workspaceFolder}/duckdb/third_party/tdigest",
        "${workspaceFolder}/duckdb/third_party/mbedtls/include",
        "${workspaceFolder}/duckdb/third_party/jaro_winkler",
        "${workspaceFolder}/duckdb/third_party/vergesort",
        "${workspaceFolder}/duckdb/third_party/yyjson/include",
        "${workspaceFolder}/duckdb/third_party/zstd/include",
        "${workspaceFolder}/duckdb/third_party/libpg_query/include",
        "${workspaceFolder}/build/release/vcpkg_installed/arm64-osx/include",
        "${workspaceFolder}/build/release/vcpkg_installed/arm64-osx/include/bsoncxx/v_noabi",
        "${workspaceFolder}/build/release/vcpkg_installed/arm64-osx/include/mongocxx/v_noabi"
    ],
    "C_Cpp.default.defines": [
        "DUCKDB_BUILD_LIBRARY",
        "BSONCXX_STATIC",
        "MONGOCXX_STATIC"
    ],
    "C_Cpp.default.compileCommands": "${workspaceFolder}/build/release/compile_commands.json",
    "C_Cpp.default.configurationProvider": null,
    "C_Cpp.default.useCompileCommands": true,
    "C_Cpp.default.intelliSenseMode": "macos-clang-arm64",
    "C_Cpp.default.cppStandard": "c++11",
    "C_Cpp.default.compilerPath": "/usr/bin/c++",
    "C_Cpp.default.name": "Mac",
    "C_Cpp.errorSquiggles": "enabled",
    "C_Cpp.intelliSenseEngine": "disabled",
    "C_Cpp.intelliSenseEngineFallback": "disabled",
    "C_Cpp.configurationWarnings": "enabled",
    "C_Cpp.autocomplete": "default",
    "C_Cpp.suggestSnippets": true,
    "C_Cpp.enhancedColorization": "enabled",
    "C_Cpp.intelliSenseUpdateDelay": 1000,
    "C_Cpp.workspaceParsingPriority": "highest",
    "C_Cpp.default.browse.limitSymbolsToIncludedHeaders": false,
    "C_Cpp.default.browse.databaseFilename": "${workspaceFolder}/.vscode/browse.vc.db",
    "C_Cpp.default.browse.path": [
        "${workspaceFolder}/src",
        "${workspaceFolder}/src/include",
        "${workspaceFolder}/duckdb/src/include",
        "${workspaceFolder}/duckdb/third_party",
        "${workspaceFolder}/build/release/vcpkg_installed/arm64-osx/include"
    ],
    "C_Cpp.default.browse.exclude": [
        "${workspaceFolder}/duckdb/src/**/test/**",
        "${workspaceFolder}/duckdb/src/**/benchmark/**",
        "${workspaceFolder}/duckdb/third_party/**/test/**",
        "${workspaceFolder}/build/**"
    ],
    "cmake.configureSettings": {
        "CMAKE_TOOLCHAIN_FILE": "${env:VCPKG_TOOLCHAIN_PATH}"
    },
    "clangd.enabled": true,
    "clangd.path": "/usr/bin/clangd",
    "clangd.arguments": [
        "--compile-commands-dir=${workspaceFolder}/build/release",
        "--background-index",
        "--clang-tidy",
        "--header-insertion=iwyu",
        "--completion-style=detailed",
        "--function-arg-placeholders=false"
    ],
    "clangd.fallbackFlags": [
        "-std=c++11"
    ],
    "clangd.onConfigChanged": "restart"
}
